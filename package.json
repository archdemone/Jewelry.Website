{
  "name": "jewelry-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "size-limit": "node scripts/check-bundle-size.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next out",
    "prebuild": "npm run type-check",
    "postbuild": "npm run size-limit",
    "web-vitals-check": "node scripts/web-vitals-guard.js",
    "pre-commit": "npm run web-vitals-check && npm run lint && npm run type-check"
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/*.js",
      "limit": "200 kB",
      "name": "First Load JS"
    },
    {
      "path": ".next/static/chunks/vendors-*.js",
      "limit": "150 kB",
      "name": "Vendor Bundle"
    },
    {
      "path": ".next/static/chunks/react-*.js",
      "limit": "50 kB",
      "name": "React Bundle"
    },
    {
      "path": ".next/static/chunks/common-*.js",
      "limit": "100 kB",
      "name": "Common Bundle"
    }
  ],
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^2.1.11",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^1.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.4",
    "jest-axe": "^10.0.0",
    "lucide-react": "^0.292.0",
    "next": "14.2.31",
    "next-auth": "^4.24.0",
    "otplib": "^12.0.1",
    "pino": "^9.9.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^3.1.2",
    "sharp": "^0.34.3",
    "stripe": "^14.0.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.31",
    "@playwright/test": "^1.40.0",
    "@size-limit/preset-app": "^11.2.0",
    "@types/jest": "^29.5.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.5",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.31",
    "image-webpack-loader": "^8.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "size-limit": "^11.0.0",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
